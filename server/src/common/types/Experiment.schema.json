          "format": "date-time",
          "type": "string"
        },
        "dataset": {
          "type": "string"
        },
        "id": {
          "type": "string"
        },
        "organisation": {
          "type": "string"
        },
        "summary_results": {},
        "updated": {
          "format": "date-time",
          "type": "string"
        }
      },
      "required": [
        "id",
        "dataset",
        "organisation",
        "summary_results",
        "created",
        "updated"
      ],
      "type": "object"
    }
